doctype html
html
    block vars
    head
        meta(charset='utf-8')
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        title #{title}

        link(rel='stylesheet', href="../../../css/style.css")
        script(type="text/javascript" src="http://api-maps.yandex.ru/2.1/?lang=ru_RU")
        script(src="../../../js/app-common.js")
        script(src="../../../js/app.js")
        script(src="../../../js/jquery.pixlayout.0.9.6.js")
        link(href='https://fonts.googleapis.com/css?family=Roboto:400,300,500,700,900,100|Lobster&subset=latin,cyrillic' rel='stylesheet' type='text/css')

    body
        img(src="../../../img/bg__grid.png" alt /).bg__decor--grid
        main
            img(src="../../../img/decor--stud-1.png" alt /).bg__decor--stud1
            img(src="../../../img/decor--stud-2.png" alt /).bg__decor--stud2
            img(src="../../../img/decor--stud-3.png" alt /).bg__decor--stud3
            img(src="../../../img/decor--stud-2-niddle.png" alt /).bg__decor--stud4
            img(src="../../../img/decor--stud-3-niddle.png" alt /).bg__decor--stud5
            .content-bg
            .content-wrap
                include header
                block content
            .psevdofooter
            include  footer